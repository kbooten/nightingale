{
	"manifest_version": 2,
	"name": "Nightingale",
	"version": "1.0",
	"description": "Literary pop-ups from Keats's 1819 odes",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["patterns.js","content_script.js"],
			"css" : ["ext_styles.css"],
			"run_at": "document_end"
		}
	],
	"icons":{
		"128":"icon128.png"
	},
	"permissions":
	[
		"storage"
	],
	"web_accessible_resources": 
	[
		"nightingale_sm.png"
	]
}
